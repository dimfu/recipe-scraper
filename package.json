{
  "name": "@dimfu/recipe-scraper",
  "version": "0.0.1",
  "description": "Extract recipe data from the web effortlessly",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "homepage": "https://github.com/dimfu/recipe-scraper",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimfu/recipe-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/dimfu/recipe-scraper/issues"
  },
  "keywords": [
    "recipe",
    "food-recipe",
    "scraper",
    "json-ld",
    "jsonld",
    "micro-data",
    "microdata"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast -- --dts-resolve",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "eslint": "^8.41.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "6.7.0",
    "typescript": "4.9.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
